{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "backend boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/index.js",
    "start:admin": "cross-env WORKER=api-admin PORT=3004 node -r ts-node/register -r tsconfig-paths/register dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "dev:admin": "cross-env WORKER=api-admin PORT=3004 ts-node-dev -r tsconfig-paths/register src/index.ts",
    "lint": "eslint src --ext .ts",
    "build": "rm -rf dist && npx tsc",
    "format": "prettier --write \"src/**/*.{ts,js}\""
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "cron": "^3.2.1",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.8.3",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.7.0",
    "shippo": "^2.14.1",
    "stripe": "^17.5.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "ts-essentials": "^10.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
